{
  "name": "pizilla",
  "version": "0.0.1",
  "description": "A file sharing web application for raspberry-pi-3",
  "main": "server/index.js",
  "scripts": {
    "clean": "rm -fr public",
    "lint": "tslint -c tslint.json '**/*.ts' --exclude 'node_modules/**/*'",
    "webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run webpack",
    "webpack:react": "webpack-dev-server --port 8080 --host localhost",
    "server": "nodemon --exec \"ts-node --cache-directory .tscache\" ./server/index.ts",
    "dev": "concurrently \"npm run webpack:react\" \"npm run server\"",
    "start": "NODE_ENV=production node public/bundle.server.js"
  },
  "keywords": [
    "file",
    "ftp",
    "raspberry"
  ],
  "author": "Naveen Kumar Sangi <nkprince007@gmail.com>",
  "repository": "https://github.com/nkprince007/PiZilla.git",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cron": "^1.2.1",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "keymirror": "^0.1.1",
    "materialize-css": "^0.100.2",
    "mime": "^2.0.3",
    "multer": "^1.3.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone-component": "^2.0.0",
    "react-fa": "^4.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/cron": "^1.2.1",
    "@types/express": "^4.0.37",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/keymirror": "^0.1.1",
    "@types/multer": "^1.3.3",
    "@types/node": "^8.0.33",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-fa": "^4.1.3",
    "@types/react-redux": "^5.0.10",
    "@types/redux-logger": "^3.0.4",
    "@types/redux-promise-middleware": "^0.0.8",
    "@types/superagent": "^3.5.6",
    "@types/webpack": "^3.0.13",
    "awesome-typescript-loader": "^3.2.3",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "file-loader": "^0.11.2",
    "nodemon": "^1.12.1",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.18.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
