{
  "name": "pizilla",
  "version": "0.0.1",
  "description": "A file sharing web application for raspberry-pi-3",
  "main": "dist/server/index.js",
  "scripts": {
    "clean": "rm -fr public && rm -fr dist",
    "lint": "eslint .",
    "server": "pm2 start dist/server/index.js --watch --name server",
    "react": "pm2 start --name react webpack-dev-server -- --config webpack.config.js",
    "build": "npm run clean && webpack --config webpack.config.prod.js && tsc",
    "dev": "npm run server | npm run react",
    "start": "NODE_ENV=production BABEL_CACHE_PATH=.cache.json node dist/server/index.js"
  },
  "keywords": [
    "file",
    "ftp",
    "raspberry"
  ],
  "author": "Naveen Kumar Sangi <nkprince007@gmail.com>",
  "repository": "https://github.com/nkprince007/PiZilla.git",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/cron": "^1.2.1",
    "@types/express": "^4.0.37",
    "@types/mime": "^2.0.0",
    "@types/multer": "^1.3.3",
    "@types/node": "^8.0.32",
    "body-parser": "^1.17.2",
    "cron": "^1.2.1",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "keymirror": "^0.1.1",
    "materialize-css": "^0.100.2",
    "mime": "^2.0.3",
    "multer": "^1.3.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone-component": "^2.0.0",
    "react-fa": "^4.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "pm2": "^2.7.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
