{
  "name": "pizilla",
  "version": "0.0.1",
  "description": "A file sharing web application for raspberry-pi-3",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=prod webpack",
    "webpack": "NODE_ENV=dev webpack-dev-server --config webpack.config.js",
    "server": "node .",
    "start": "concurrently \"npm run webpack\" \"npm run server\""
  },
  "keywords": [
    "file",
    "ftp",
    "raspberry"
  ],
  "author": "Naveen Kumar Sangi <nkprince007@gmail.com>",
  "repository": "https://github.com/nkprince007/PiZilla.git",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
